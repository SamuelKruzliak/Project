<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Project.MainCalculation">
    <ContentPage.Content>
        
        <StackLayout x:Name="FinalResults">
            <Frame x:Name="infoFrame" CornerRadius="15">
                <StackLayout>
                    <Label x:Name="SubnetMask" FontSize="25"></Label>
                    <Label x:Name="NumOfHosts" FontSize="25"></Label>
                </StackLayout>
            </Frame>
            <Label x:Name="SubnetsSign"></Label>

        </StackLayout>
    </ContentPage.Content>
</ContentPage>